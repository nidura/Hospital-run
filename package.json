        "eject": "cross-env NODE_PATH=src react-scripts eject",
        "build-docs": "babel-node src/main/content/components/material-ui/build.js",
        "build:DEV": "yarn run build-css-helpers && cross-env NODE_PATH=src REACT_APP_ENVIRONMENT=DEV react-scripts --max_old_space_size=6144 build",
        "build:QA": "yarn run build-css-helpers && cross-env NODE_PATH=src REACT_APP_ENVIRONMENT=QA react-scripts --max_old_space_size=6144 build",
        "build:ZINCAT_LIVE": "yarn run build-css-helpers && cross-env NODE_PATH=src REACT_APP_ENVIRONMENT=ZINCAT_LIVE react-scripts --max_old_space_size=4096 build",
        "start:DEV": "yarn run build-css-helpers && cross-env NODE_PATH=src REACT_APP_ENVIRONMENT=DEV react-scripts start",
        "start:QA": "yarn run build-css-helpers && cross-env NODE_PATH=src REACT_APP_ENVIRONMENT=QA react-scripts start",
        "start:ZINCAT_LIVE": "yarn run build-css-helpers && cross-env NODE_PATH=src REACT_APP_ENVIRONMENT=ZINCAT_LIVE react-scripts start",
